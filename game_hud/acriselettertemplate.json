    // RiseLetter_Gold Group
    {
      "TemplateName": "RiseLetter_Gold", // 名称，名称必须用这个. 这个跟服务器发送的跳字类型有关
      "Anchor": [ 0, 0 ], // 锚点位置
      "AddTimeLimit": 0, // 可累加的时间段限制, 在这个时间内新收到的同类型的跳字事件，会复用当前跳字
      "Layout": // Widget布局(请参考 血条模板)
      [
        // 金币数字
        {
          "Name": "Content", // 名字必须是这个
          "Type": "TEXT", // 控件类型
          "Rect": [ 0, 0, 255, 24 ], // X, Y, Width, Height
          "Color": [ 255, 255, 255, 255 ], // 颜色rgba， 会与字体的颜色做乘法
          "PreText": "$", // 前缀字符,  最终显示的字符串为 $888888
          "Text": "888888", // 可不用填写
          "Font": "金币", // 字体: 这个字体可以填写UE3中的字体对象名称，也可以填写用户自定义的字体名称
          "Align": 0 // 0-左对齐, 1-居中, 2-右对
        }
      ],
      "AnimationSet": // 动画列表(每次跳字事件发生 会随机选择一个播放)
      [
        // 动画0
        {
          "Name": "01", // 名字可不设置
          "Sequence": // 每个动画由多个动作序列组成
          [
            // 动作01
            {
              "Time": 0.6, // seconds. 该动作时长为0.6秒，  该跳字从Start到End耗时0.6秒, 其位置和其它属性(color, Alpha)是线性插值过渡
              "Curve": {
                "Type": "Line", // 直线类型
                "Ctrls": [
                  {
                    "X": -10,
                    "Y": -82
                  },
                  {
                    "X": -10,
                    "Y": -138
                  }
                ] // 2个控制点(也就是直线的2个端点AB)
              },
              "Start": // 起点
              {
                "T": 0.0, // t为上面控制点的权重值, 0.0表示从端点A开始
                "Alpha": 255 // alpha值
              },
              "End": // 终点
              {
                "T": 1.0, // 1.0表示端点B
                "Alpha": 255
              }
            },
            // 动作02
            {
              "Time": 0.3, // 0.3 seconds.
